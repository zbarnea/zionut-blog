<h2>Latest Articles</h2>
<br>

<p id="notice"><%= notice %></p>

<h1>Articles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <%= link_to article.title, article %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article', new_article_path %>